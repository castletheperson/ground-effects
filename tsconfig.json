{
  "compilerOptions": {
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "outDir": "static/js",
    "sourceMap": true,
    "strict": true,
    "ignoreDeprecations": "5.0",
    "target": "es3",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "downlevelIteration": true
  },
  "typeRoots": [
    "node_modules/@types",
  ],
  "compileOnSave": true,
  "include": [
    "src/scripts"
  ],
}
