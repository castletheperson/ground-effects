{{#> layouts/base title="Project Gallery" }}

  {{#*inline "head-block" }}
    <link rel="stylesheet" href="css/gallery.css" type="text/css">
    <link rel="stylesheet" href="images/photoswipe/photoswipe.css" type="text/css">
    <link rel="stylesheet" href="images/photoswipe/default-skin/default-skin.css" type="text/css">
  {{/inline}}

  {{#*inline "heading" }}
    <div class="text-center">
      <h6>Project</h6>
      <h2>Gallery</h2>
    </div>
    <br>
  {{/inline}}
  
  {{#*inline "content" }}
    <div id="gallery" class="container">
      <div class="masonry" id="gallery-images" itemscope itemtype="http://schema.org/ImageGallery">
        {{#*inline "image" }}
          <figure itemprop="associatedMedia" itemscope itemtype="http://schema.org/ImageObject">
            <a href="images/gallery/{{src}}"
              itemprop="contentUrl"
              data-src="images/gallery/{{src}}"
              data-msrc="images/thumbnails/tn_{{src}}"
              data-w="{{w}}"
              data-h="{{h}}">
              <img src="images/thumbnails/tn_{{src}}" itemprop="thumbnail" alt="Gallery Image">
            </a>
          </figure>
        {{/inline}}

